      ]
    },
    {
      "id": "mulkiya_front",
      "question": "Please upload your Mulkiya Front Page",
      "type": "file_upload",
      "required": true,
      "dependsOn": {
        "id": "insurer",
        "value": ["Alsagar","DNI","Methaq"]
      },
